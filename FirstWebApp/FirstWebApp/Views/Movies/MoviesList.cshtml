@model FirstWebApp.ViewModels.MoviesListViewModel

@{ 
    ViewBag.Title = "MoviesList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Movies List</h3>

<div class="container">
    <p></p>
    <table id="clientsTableList" class="table table-bordered table-hover">
        <tr>
            <th><h6><strong>Movie Name</strong></h6></th>
        </tr>
        @foreach (var movie in Model.Movies)
        {
            <tr>
                <td>@movie.Name</td>
            </tr>
        }

    </table>
</div>

